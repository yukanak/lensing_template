#!/bin/bash
#SBATCH --job-name=rotate_agora
#SBATCH --time=24:00:00
#SBATCH --array=4
#SBATCH --cpus-per-task=12
#SBATCH --mem=128G
#SBATCH --partition=kipac
#SBATCH --constraint=CLASS:SH4_CSCALE

export OMP_NUM_THREADS=12

lmax=16000

python rotate_agora.py 95 1001 $SLURM_ARRAY_TASK_ID ${lmax}
python rotate_agora.py 150 1001 $SLURM_ARRAY_TASK_ID ${lmax}
python rotate_agora.py 220 1001 $SLURM_ARRAY_TASK_ID ${lmax}
